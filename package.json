{
  "name": "personal_site",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://polymer940c.github.io/",
  "dependencies": {
    "gh-pages": "^1.2.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-transition-group": "^2.4.0",
    "splitting": "^1.0.5"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "predeploy": "npm run build",
    "deploy": "master -d build",
    "eject": "react-scripts eject",
    "build-js": "react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test --env=jsdom",
    "build-css": "node-sass-chokidar ./src/styles/index.scss ./src/styles/index.css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/index.scss ./src/styles/index.css --watch --skip-initial"
  },
  "devDependencies": {
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0"
  }
}